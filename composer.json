{
  "name": "shemi/laradmin",
  "description": "A Laravel Admin Package",
  "license": "MIT",
  "authors": [
    {
      "name": "shemi perez",
      "email": "shemi.perez@gmail.com"
    }
  ],

  "require": {
    "php" : ">=7.0",
    "illuminate/bus": "~5.5.0",
    "league/flysystem": "^1.0.8",
    "illuminate/console": "~5.5.0",
    "illuminate/database": "~5.5.0",
    "illuminate/support": "~5.5.0",
    "spatie/laravel-permission": "^2.4",
    "spatie/laravel-medialibrary": "^6.0",
    "easybook/slugger": "1.*",
    "justinrainbow/json-schema": "^5.2",
    "illuminate/contracts": "~5.5"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/database": "~3.5",
    "orchestra/testbench": "~3.5",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Shemi\\Laradmin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shemi\\Laradmin\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "*"
      ],
      "providers": [
        "Shemi\\Laradmin\\LaradminServiceProvider"
      ],
      "aliases": {
        "Laradmin": "Shemi\\Laradmin\\Facades\\Laradmin"
      }
    }
  }
}
